		{
			"name": "Создание пользователя",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Получаем текущее окружение\r",
							"let environment = pm.environment.name;\r",
							"\r",
							"// Функция генерации случайного имени\r",
							"function generateName() {\r",
							"    const characters = \"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ\";\r",
							"    const length = Math.floor(Math.random() * 8) + 3; // Длина от 3 до 10 символов\r",
							"    let name = \"\";\r",
							"    for (let i = 0; i < length; i++) {\r",
							"        name += characters.charAt(Math.floor(Math.random() * characters.length));\r",
							"    }\r",
							"    return name;\r",
							"}\r",
							"\r",
							"// Функция генерации случайного email\r",
							"function generateEmail(firstName, lastName) {\r",
							"    const domains = [\"gmail.com\", \"yahoo.com\", \"outlook.com\"];\r",
							"    let domain = domains[Math.floor(Math.random() * domains.length)];\r",
							"    return `${firstName.toLowerCase()}.${lastName.toLowerCase()}@${domain}`;\r",
							"}\r",
							"\r",
							"// Функция генерации случайного титула\r",
							"function generateTitle() {\r",
							"    const titles = [\"mr\", \"mrs\", \"miss\", \"dr\"];\r",
							"    const randomIndex = Math.floor(Math.random() * titles.length);\r",
							"    return titles[randomIndex];\r",
							"}\r",
							"\r",
							"// Функция генерации случайного URL картинки\r",
							"function generatePicture() {\r",
							"    const genders = [\"men\", \"women\"];\r",
							"    const randomGender = genders[Math.floor(Math.random() * genders.length)];\r",
							"    const randomId = Math.floor(Math.random() * 100);\r",
							"    return `https://randomuser.me/api/portraits/${randomGender}/${randomId}.jpg`;\r",
							"}\r",
							"\r",
							"// Генерация данных\r",
							"const firstName = generateName();\r",
							"const lastName = generateName();\r",
							"const title = generateTitle();\r",
							"const picture = generatePicture();\r",
							"\r",
							"// Проверяем окружение\r",
							"let email;\r",
							"if (environment === \"QA\") {\r",
							"    email = generateEmail(firstName, lastName); // Генерация email в QA\r",
							"} else {\r",
							"    email = pm.environment.get(\"email\") || \"default@example.com\"; // Берем из переменной или ставим дефолт\r",
							"}\r",
							"\r",
							"// Установка переменных для тела запроса\r",
							"pm.environment.set(\"firstName\", firstName);\r",
							"pm.environment.set(\"lastName\", lastName);\r",
							"pm.environment.set(\"email\", email);\r",
							"pm.environment.set(\"title\", title);\r",
							"pm.environment.set(\"picture\", picture);\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							"\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
